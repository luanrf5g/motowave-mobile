{
  "expo": {
    "name": "MotoWave",
    "slug": "mobile-motowave",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#121212"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kytsoftwares.motowave",
      "infoPlist": {
        "UIBackgroundModes": [
          "location",
          "fetch"
        ],
        "NSLocationAlwaysAndWhenInUseUsageDescription": "O MotoWave precisa do seu GPS sempre ativo para gravar sua rota enquanto o celular est√° no bolso.",
        "NSLocationAlwaysUsageDescription": "Rastreamento de rota em segundo plano.",
        "NSLocationWhenInUseUsageDescription": "Rastreamento de rota."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#121212"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.kytsoftwares.motowave",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCK95wV_UFNtlkUwE4LumXC26A7y_ESCW0"
        }
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "FOREGROUND_SERVICE",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-maps",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow MotoWave to use your location."
        }
      ],
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "d1a7004c-f3b2-4c44-8d6d-18b261e87444"
      }
    }
  }
}